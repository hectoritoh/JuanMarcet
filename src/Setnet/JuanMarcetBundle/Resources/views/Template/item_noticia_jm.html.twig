<div class="row" style="margin: 10px;cursor:pointer;"    onclick="window.location='{{  path('noticia' , {id_noticia :  noticia.id  }) }}'" >
    <div class="span3" style="margin-left: 0px;width:100%;background-color:#ffffff;margin-bottom:12px;">
        <ul style="list-style:none outside none; margin-left: 0px;position: relative">
            <li  style="margin-left: 0px;">
                <div style="margin-left:10px;">
                	<h3 class="label_subtitulo_noticias_jm">{{  noticia.titulo }}</h3>
	                <h6 style="margin: 0px;color: #707070;font-family: Arial;line-height: 2px;">{{  noticia.created | date('l jS F Y ') }}</h6>
                </div>
                <img style="width: 100%;height: 133px;margin-top:26px;" src="{{ asset('uploads/noticias/'  ~ noticia.imagen  ) }}" alt="">
            </li>
        </ul>
        <div class="separador"></div>
    </div>
</div>